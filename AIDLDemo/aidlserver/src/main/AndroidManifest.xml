<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.codekong.aidlserver">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <service
            android:name=".service.SinaSsoService"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.codekong.aidlserver.service.SinaSsoAuthService"/>
            </intent-filter>
        </service>
        <service android:name=".service.AidlService">
            <intent-filter>
                <action android:name="com.codekong.aidlservice.service.aidl"/>
            </intent-filter>
        </service>
    </application>

</manifest>